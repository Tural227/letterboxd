<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="CornerCut">
        <item name="cornerSize">18dp</item>

        <item name="cornerFamily">rounded</item>
    </style>

    <style name="BottomNavigationView" parent="ShapeAppearanceOverlay.Material3.NavigationView.Item">
        <item name="android:icon">@drawable/item_selected</item>
    </style>

    <style name="very_custom_rating_bar" parent="Widget.AppCompat.RatingBar">
        <item name="android:progressDrawable">@drawable/custom_rating_bar</item>
        <item name="minHeight">48dp</item>
        <item name="minWidth">48dp</item>
    </style>


    <style name="MyTabTextAppearence" parent="TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse">
        <item name="fontFamily">@font/open_sans_semibold</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textAllCaps">false</item>
    </style>


    <style name="InvisibleIndicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:color">#00000000</item>
    </style>

</resources>